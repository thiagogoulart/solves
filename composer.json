{
    "name": "thiagogoulart/solves",
    "description": "Classes úteis para Autorização, Pagamento, Envio de E-mail, WebSocket, Router, Mapeamento Objeto relacional de forma simples e configurável para PHP, entre outras classes úteis.",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "pagseguro/pagseguro-php-sdk" : "6.0.0",
        "kreait/firebase-php" : "4.39.2",
        "minishlink/web-push": "v5.2.4",
        "phpmailer/phpmailer" : "v6.1.4",
        "cboden/ratchet": "v0.4.2",
        "robmorgan/phinx": "0.11.4"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:pagseguro/pagseguro-php-sdk.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:kreait/firebase-php.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/PHPMailer/PHPMailer.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/web-push-libs/web-push-php.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ratchetphp/Ratchet.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/robmorgan/phinx.git"
        }
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Thiago G.S. Goulart",
            "email": "thiago@solves.com.br",
            "homepage": "https://github.com/thiagogoulart"
        }
    ],
    "autoload": {
        "psr-4": {
            "Solves\\": "src/Solves/",
            "SolvesAuth\\": "src/SolvesAuth/",
            "SolvesDAO\\": "src/SolvesDAO/",
            "SolvesMail\\": "src/SolvesMail/",
            "SolvesPay\\": "src/SolvesPay/",
            "SolvesUi\\": "src/SolvesUi/",
            "SolvesNotification\\": "src/SolvesNotification/",
            "SolvesWebsocket\\": "src/SolvesWebsocket/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "bin": [
        "bin/solves/"
    ]
}
